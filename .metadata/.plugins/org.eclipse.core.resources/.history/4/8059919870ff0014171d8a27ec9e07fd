import java.awt.Color;
import java.util.Random;


public class Ball {
	
	int x;
	int y;
	int w;
	int h;
	
	
	
	public Ball(int x, int y, int w, int h) {
		this.x = x;
		this.y = y;
		this.w = w;
		this.h = h;
	}

	public void update(){
		
	}

	public int getX() {
		return x;
	}

	public int getY() {
		return y;
	}

	public int getW() {
		return w;
	}

	public int getH() {
		return h;
	}


}